    <record model="ir.ui.view" id="product_fixed_prices_inherit_form">
      <field name="name">product.fixed.prices.inherit.form</field>
      <field name="inherit_id" ref="product.product_template_only_form_view" />
      <field name="model">product.template</field>
      <field name="arch" type="xml">
        <notebook>
          <page string="Listas de Precios ">
            <group cols="2">
              <field name="currency_fixed_id" required="1"
                options='{"no_create": True, "no_quick_create": True,"no_open":True}' />

            </group>
              <field name="fixed_list_price_ids">
                <tree >
                  <field name="list_num" />
                  <field name="margin_pctg" groups="fixed_prices.cost_and_price" />
                  <field name="price" />
                </tree>
              </field>

          </page>
        </notebook>

      </field>
    </record>


    <record model="ir.ui.view" id="partner_fixed_prices_inherit_form">
      <field name="name">partner.fixed.prices.inherit.form</field>
      <field name="inherit_id" ref="base.view_partner_form" />
      <field name="model">res.partner</field>
      <field name="arch" type="xml">
        <notebook>
          <page string="Listas de Precios Govar">
            <group cols="4">
              <field name="list_num" />
            </group>
          </page>
        </notebook>
      </field>
    </record>

  </data>
</odoo>